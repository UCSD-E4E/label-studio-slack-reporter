    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Service",
            "type": "debugpy",
            "request": "launch",
            "module": "label_studio_slack_reporter.service",
            "args": [
                "--config",
                "${workspaceFolder}/config.toml",
                "--debug"
            ]
        },
        {
            "name": "Run Launch",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/label_studio_slack_reporter/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--config",
                "${workspaceFolder}/config.json"
            ]
        }
    ]
}